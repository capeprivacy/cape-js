    "@types/elliptic": "^6.4.14",
    "@types/node-forge": "^1.0.4",
    "@types/pkijs": "^0.0.14",
    "buffer": "^6.0.3",
    "cbor": "^8.1.0",
    "cbor-rn-prereqs": "^8.1.0",
    "cbor-web": "^8.1.0",
    "cose-js": "^0.8.4",
    "elliptic": "^6.5.4",
    "node-forge": "^1.3.1",
    "pkijs": "^3.0.5"
  },
  "devDependencies": {
    "@types/cose-js": "^0.8.0"
  }
